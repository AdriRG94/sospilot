[
  {% for timeserie in timeseries %}
  {
    "id": "{{ timeserie['id'] }}",
    "label": "{{ timeserie['label'] }} - {{ timeserie['device_name'] }}",
    "uom": "{{ timeserie['unit'] }}",
    "station": {
      "properties": {
        "id": {{ timeserie['device_id'] }},
        "label": "{{ timeserie['device_name'] }}"
      },
      "geometry": {
        "coordinates": [
          {{ timeserie['lon'] }},
          {{ timeserie['lat'] }}
        ],
        "type": "Point"
      },
      "type": "Feature"
    },
    "referenceValues": [],
    "firstValue": {
      "timestamp": {{ timeserie['timestamp'] }},
      "value": {{ timeserie['sample_value'] }}
    },
    "lastValue": {
      "timestamp": {{ timeserie['timestamp'] }},
      "value": {{ timeserie['sample_value'] }}
    },
    "parameters": {
      "service": {
        "id": "1",
        "label": "Smart Emission: timeseries-api (SOS 2.0)"
      },
      "offering": {
        "id": "{{ timeserie['id'] }}",
        "label": "{{ timeserie['id'] }} - src={{ timeserie['name'] }} - src_value={{ timeserie['value_raw'] }}  - offering"
      },
      "feature": {
        "id": "{{ timeserie['device_id'] }}",
        "label": "{{ timeserie['device_name'] }}"
      },
      "procedure": {
        "id": "{{ timeserie['id'] }}",
        "label": "{{ timeserie['id'] }} - on {{ timeserie['sample_time'] }} - procedure"
      },
      "phenomenon": {
        "id": "{{ timeserie['id'] }}",
        "label": "{{ timeserie['label'] }}"
      },
      "category": {
        "id": "{{ timeserie['id'] }}",
        "label": "{{ timeserie['label'] }}"
      }
    },
    "statusIntervals": []
  }

  {% if not loop.last %}
      ,
  {% endif %}
  {% endfor %}
]
